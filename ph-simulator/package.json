{
  "name": "ph-simulator",
  "version": "0.0.1",
  "private": true,
  "main": "src/electron.cjs",
  "scripts": {
    "dev": "vite dev",
    "dev:all": "concurrently -n=svelte,electron -c='#ff3e00',blue \"bun run dev\" \"bun run dev:electron\"",
    "dev:electron": "electron src/electron.cjs",
    "build": "vite build",
    "build:all": "bun run build && bun run build:electron",
    "build:electron": "cross-env NODE_ENV=production electron-builder -mwl --config build.config.json",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@zerodevx/svelte-toast": "^0.9.5",
    "autoprefixer": "^10.4.17",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^29.1.0",
    "electron-builder": "^24.13.3",
    "electron-connect": "^0.6.3",
    "electron-packager": "^17.1.1",
    "electron-updater": "^5.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.35",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "svelte-preprocess": "^5.0.3",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
    "bits-ui": "^0.19.3",
    "clsx": "^2.1.0",
    "electron-context-menu": "^3.6.1",
    "electron-reloader": "^1.2.3",
    "electron-serve": "^1.1.0",
    "electron-window-state": "^5.0.3",
    "lucide-svelte": "^0.343.0",
    "tailwind-merge": "^2.2.1",
    "tailwind-variants": "^0.2.0"
  }
}
